{
  "manifest_version": 2,
  "name": "Clip",
  "author": "SpicyKimChoi",
  "version": "1.0.1",
  "description": "clip helps your co-work",

  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": ["tabs"],
  "content_security_policy": "script-src 'self' https://apis.google.com; object-src 'self'"
}
